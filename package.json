{
  "name": "kimi-k2.5-client",
  "version": "1.0.0",
  "description": "Kimi K2.5 Client with Agent Swarm Support",
  "main": "dist/kimi_client.js",
  "types": "dist/kimi_client.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node kimi_client.ts",
    "dev": "ts-node-dev --respawn kimi_client.ts",
    "example:simple": "ts-node examples/simple_chat.ts",
    "example:swarm": "ts-node examples/agent_swarm_demo.ts",
    "example:research": "ts-node examples/research_swarm.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\""
  },
  "keywords": [
    "kimi",
    "ai",
    "agent-swarm",
    "multimodal",
    "moonshot",
    "ollama"
  ],
  "author": "Andrew Morton",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.14.0",
    "dotenv": "^16.3.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "pg": "^8.18.0",
    "playwright": "^1.58.2",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
